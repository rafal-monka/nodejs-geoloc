  "bugs": {
    "url": "https://github.com/rafal-monka/nodejs-geoloc/issues"
  },
  "homepage": "https://github.com/rafal-monka/nodejs-geoloc#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-handlebars": "^4.0.4",
    "moment": "^2.26.0",
    "mongodb": "^3.5.8",
    "mongoose": "^5.9.17",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
